	<Script><![CDATA[#include "lib::commons.js";		// Form 공통 Script

function GridFilterSample_OnLoadCompleted(obj)
{
	grd_fn_PopupDivFilterInit(grd_Left.id);
	grd_fn_PopupDivFilterInit(grd_Right.id);
}

function btn_gridAdd_OnClick(obj)
{
	var nRow = ds_Left.AddRow();
