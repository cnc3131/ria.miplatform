function grd_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	// Head가 Filter 상태 일때 처리
	if(	obj.GetCellProp("head",nCell,"expandimage") == GRD_STR_UNFILTER_IMAGE ||
				obj.GetCellProp("head",nCell,"expandimage") == GRD_STR_FILTER_IMAGE ) {

		grd_fn_PopupDivFilter(obj,nCell);
	}
}

function Button0_OnClick(obj)
{
	grd_fn_PopupDivFilterSet(grd_Left);
}

function Button1_OnClick(obj)
{
	grd_fn_PopupDivFilterSet(grd_Right);
}
]]></Script>
