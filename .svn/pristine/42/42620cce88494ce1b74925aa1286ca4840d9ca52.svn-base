function img_menu_OnClick(obj,nX,nY)
{
	var menuRow = gds_menu.FindRow("MENUNAME", obj.text );
	
	if(menuRow <0) return;
	gds_menu.row  = menuRow;
	
	global.Frame_Left.pdiv_menu.Open_url( menuRow );
