function gfn_menufind(arg_menuid)
{
	for ( wi = 0; wi < global.windows.count ; wi ++)
	{
		if (global.windows[wi].id == arg_menuid) 
		{
			global.windows[wi].SetFocus();
			//global.windows[wi].fn_LoadMenu(arg_value1);
			return false;
		}
	}
	return true;
}


// Open 메뉴 추가...
function gfn_OpenMenuAdd( str_prefix, str_url, str_title, str_formid )
{
	var result, MacCnt;
	var nRow;
	
	nRow = gds_OpenMenu.FindRow("page_id", str_formid);
	if( nRow >= 0 )
	{
		return;
	} else
	{
		nRow = gds_OpenMenu.AddRow();
		
		gds_OpenMenu.SetColumn(nRow,"page_id", str_formid );
		//gds_OpenMenu.SetColumn(nRow,"page_nm", str_title );
		gds_OpenMenu.SetColumn(nRow,"page_prefix", str_prefix );
		gds_OpenMenu.SetColumn(nRow,"page_url", str_url );
		
		if( str_url == str_formid )
			gds_OpenMenu.SetColumn(nRow,"page_nm", str_title);
		else
			gds_OpenMenu.SetColumn(nRow,"page_nm", str_title + "(조회)" );
		
				
		Global.Frame_Bottom.fn_tab_add(str_url, str_title);

		
		//Global.Doc_msg2.fn_tab_add(str_url,str_title);
	}
}

// Open 메뉴 삭제...
function gfn_OpenMenuDel( str_url )
{
	var nRow;
	
	//trace("g_OpenMenuDel str_url : " + str_url );
	
	nRow = gds_OpenMenu.FindRow("page_id", str_url);
	if( nRow >= 0 )
	{
		gds_OpenMenu.DeleteRow(nRow);
		//Global.Doc_msg.st_OpenForm.Text = gds_OpenMenu.rowcount + " 개의 폼이 실행되고 있습니다.";
		Global.Doc_msg.fn_tab_del(str_url);
	}
}

