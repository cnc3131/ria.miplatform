			return;
		}
		
		/*
		if( Doc_top.chkNewWindow.Value == true )
		{
			Open("pub_main::work_form_w.xml", ArgStr, 995, 545, "OpenStyle=Normal,Resize=true,TaskBar=true", 0, 0);
		} 
		else
		{
			if( str_prefix == "IE" )
				NewWindow(str_formid, "pub_main::work_form_ie.xml", ArgStr, 995, 545, "OpenStyle=Normal,Instance=Single", 0, 0);
			else
				NewWindow(str_formid, "pub_main::work_form_w.xml", ArgStr, 995, 545, "OpenStyle=Normal,Instance=Single", 0, 0);
		}
		//NewWindow(str_formid, "pub_main::work_form.xml", ArgStr, 775, 578, "OpenStyle=Normal,Instance=Single", 0, 0);
		//NewWindow(str_formid, "pub_main::work_form_notitle.xml", ArgStr, 995, 545, "OpenStyle=Normal,Instance=Single,Title=false,OverLapped=false", 0, 0);
		*/
		
		var strUrl = str_prefix + "::" + str_url;
		
