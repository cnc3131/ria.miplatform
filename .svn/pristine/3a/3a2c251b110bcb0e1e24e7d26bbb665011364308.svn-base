		ArgStr += " arg_Title=" + quote(str_title);
		ArgStr += " arg_Size=" + quote(str_size);
		ArgStr += " arg_Auth=" + quote(str_Auth);
		ArgStr += " arg_menuid=" + quote(str_menuid);
		ArgStr += " arg_formid=" + quote(str_url);
		
	if ((str_prefix != null) && (str_url != null)) 
