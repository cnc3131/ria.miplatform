	<Script><![CDATA[

function Global_Error(obj,nErrorCode,strErrorMsg)
{
	// ESC key Down시 수행중인 통신을 중단을 막는 기능입니다.
	// Global의 EnforcedIgnorInput 속성을 True로 설정, OnError Event에 하기(red,Bold) 내용을 적용하면 됩니다.

	trace("[f_GlobalOnError]");
	trace("nErrorCode : " + nErrorCode);
	trace("strErrorMsg : " + strErrorMsg);
	
	// ESC LOCK
	if(toNumber(nErrorCode) == -2085612799) {
		return true;
	}
}

function fn_data(str_msg)
{
	Doc_bottom.fn_setMessage("aaa");
}


function Global_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam)
{
	trace("Global KeyDown : " + bControl + "/" + nChar);
	
	if( bControl == 1 )
	{
		if( nChar == "113" )
		{
			// F2
			Doc_top.fn_menufixcontrol();
		}
	} 
	else if( nChar == "113" )
	{
