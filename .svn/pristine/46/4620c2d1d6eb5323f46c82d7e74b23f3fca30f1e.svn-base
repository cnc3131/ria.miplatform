function fn_GridRadioClick(obj,nRow,nCell,nX,nY,radioColCnt)
{
	var arrRect = obj.GetCellRect(nRow,nCell);
	var nColIdx = obj.GetCellProp("body",nCell,"col");
	var nLeftPos;
	var nTopPos;
	var nRightPos,nBottomPos;
	var i;
	var nSubColIdx,nSubRowIdx;
	var arrColWidth = array();
	var ret;
	
	for ( i = 0 ; i < obj.GetSubCellCount("body",nCell) ; i++ )
	{
		nSubColIdx = obj.GetSubCellProp("body",nCell,i,"col");
		nSubRowIdx = obj.GetSubCellProp("body",nCell,i,"row");
		
		if ( nSubRowIdx == 0 )
			nTopPos = arrRect[1];
		else
			nTopPos = arrRect[1] + obj.RowHeight * nSubRowIdx;
			
		nBottomPos = nTopPos + obj.RowHeight;
		nRightPos = 0;
		ret	= fn_arrTotalSum(obj,nColIdx,arrColWidth,nSubColIdx);
		arrColWidth = ret[1];
		nRightPos = ret[0] + arrRect[0];;
		
		//nRightPos = arrRect[0] + fn_arrTotalSum(obj,nColIdx,ref(arrColWidth),nSubColIdx);
		if ( nSubColIdx == 0 )
			nLeftPos = arrRect[0];
		else
			nLeftPos = nRightPos - arrColWidth[nSubColIdx - 1];
			
		if ( ( nLeftPos <= nX ) && ( nRightPos >= nX ) &&
		     ( nTopPos <= nY ) && ( nBottomPos >= nY ) )
		 break;
	}	
	
	return nSubRowIdx * radioColCnt + nSubColIdx;
}

function fn_arrTotalSum(obj,nColIdx,arrWidth,SubColIdx)
{
	var arrRet = array();
	var nTmp = 0;
	for ( var i = SubColIdx ; i >= 0 ; i-- )
	{
		if ( arrWidth[i] > -1 )
		{
			nTmp += arrWidth[i];
			continue;
		}	
		
		arrWidth[i] = obj.GetColProp(nColIdx + i,"Width");
		nTmp += arrWidth[i];
	}
	
	arrRet[0] = nTmp;
	arrRet[1] = arrWidth;
	return arrRet;
}


