	<Script><![CDATA[//#include "lib::common.js";

var beforeRow = 0;

function form_OnLoadCompleted(obj)
{
	var args = "searchCondition="+cb_searchCondition.Value
			+ " searchKeyword="+ed_searchKeyword.Text;

	transaction("list", 
				"svcLotte::sample/selectSampleList.mip", 	// 	http://127.0.0.1:8080/web/sample/lotteSampleList.mip
				"", 
				"ds_list=dsOutput", 
				args, 
				"fnCallbackLotte");
}

// 조회.
function Button0_OnClick(obj)
{
	transaction("list", 
				"svcLotte::sample/selectSampleList.mip", 	// 	http://127.0.0.1:8080/web/sample/lotteSampleList.mip
				"", 
				"ds_list=dsOutput", 
				"", 
				"fnCallbackLotte");
}
// 검색 조회.
function btn_search_OnClick(obj)
{
	var args = cb_searchCondition.Value + "="+ed_searchKeyword.Text;

	transaction("list", 
				"svcLotte::sample/selectSampleList.mip", 	// 	http://127.0.0.1:8080/web/sample/lotteSampleList.mip
				"", 
				"ds_list=dsOutput", 
				args, 
				"fnCallbackLotte");
}

// 등록.
function Button1_OnClick(obj)
{
	beforeRow = ds_list.row;
	var ret = Dialog("DefApp::lotteSampleRegist.xml", "", 800, 543, "Autosize=false resize=false");
	if ( ret == "0" )
	{
		ds_list.row = beforeRow;
		Button0.Click(); 
	}
}

// 수정.
function grid_list_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	var param = ds_list.GetColumn(nRow, "id");
	
	var ret = Dialog("DefApp::lotteSampleUpdate.xml", "param="+Quote(param), 800, 543, "Autosize=false resize=false");
	if ( ret == "0" )
	{
		beforeRow = ds_list.row;
		form_OnLoadCompleted();
	}
}

// 엑셀.
function btnExcel_OnClick(obj)
{
	Grid0.Format = "format0";
	Grid0.ExportExcelEx("sheet1");
}

// 변수를 VO형태로 전송 조회ㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡㅡ
function btn_searchByVO_OnClick(obj)
{
	ds_param.ClearData();
	var row = ds_param.AddRow();
	ds_param.SetColumn(row, cb_searchCondition.Value	, ed_searchKeyword.Text);

	transaction("listByVo", 
				"sample/selectSampleList.mip", 
				"dsInput=ds_param:U", 
				"ds_list=dsOutput", 
				"", 
				"fnCallbackLotte");
}

// 후처리.
function fnCallbackLotte(svcid, errcd, errmsg)
{
	if ( svcid == "list" )
	{
	
		this.status = "  : " + errmsg;
		trace(http.SendHttpStr);
		trace(http.RecvHttpStr);
		
		ds_list.row = beforeRow;
	}
}


]]></Script>
