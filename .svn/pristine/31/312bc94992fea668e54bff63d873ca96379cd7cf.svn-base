function CyXChart0_Load()
{
	var mon_cnt = ds_score_level.GetColCount();

	CyXChart0.TopGap = "30";						//차트 안쪽 상단 여백
	CyXChart0.LeftGap = "15";						//차트 안쪽 왼쪽 여백
	//ChartFx0.RightGap = "30";					//차트 안쪽 오른쪽 여백
	CyXChart0.BottomGap = "25";					//차트 안쪽 하단 여백

	CyXChart0.ClearData(1);						//Clear all numeric values set with the YValue or Value properties
	
	CyXChart0.Stacked = 0;

	//6.2 추가 사항
	CyXChart0.PointLabels = True;
	CyXChart0.PointLabelColor = "black";


	//Chart 외곽 영역 색상
	CyXChart0.BackColor = "#f2f2fe";	
	
	//Chart 내부 색상
	CyXChart0.InsideColor = "#ffffff";				
	
	
	//Grid 가로선 색상
	CyXChart0.AxisY.Grid.Color = "#C7C8D4";		
	//Grid 가로선 종류
	CyXChart0.AxisY.Grid.Style = "DashStyle_Dash";
    
    //6.2 추가
    CyXChart0.AxisY.Gridlines = True;
    CyXChart0.AxisX.Gridlines = True;

	//Max Value Setting
    CyXChart0.AxisY.Max = 200 * 1.25;		//ds_score_level.Max("col1") * 1.25;
    CyXChart0.AxisY.Min = 0;	//Min 값 을 반드시 지정해야 함

	//소수점 자리수 - Primary
	CyXChart0.AxisY.LabelsFormat.Decimals = 0;
	CyXChart0.AxisY.LabelsFormat.Format = "AxisFormat_Number";
	CyXChart0.AxisY.AutoScale = False;
	//Max Value Setting
    CyXChart0.AxisY2.Max = 200 * 1.25;			// ds_score_level.Max("col3") * 1.1;
    CyXChart0.AxisY2.Min = 0;	//Min 값 을 반드시 지정해야 함

	//소수점 자리수 - Secondary
	CyXChart0.AxisY2.LabelsFormat.Decimals = 0;
	CyXChart0.AxisY2.LabelsFormat.Format = "AxisFormat_Number";
	CyXChart0.AxisY2.AutoScale = False;

	//X축 Font
	CyXChart0.AxisX.Font.Name = "Arial";
	CyXChart0.AxisX.Font.Size = "7";


	CyXChart0.MarkerShape = 2;
	CyXChart0.Scrollable = 1;

	CyXChart0.Axis[2].Scroll = 0;
	CyXChart0.Axis[2].pixperunit = 30;

	//X축의 갯수 설정
	//가운데 숫자가 시리즈의 갯수를 나타냄, 마지막 값은 X축 갯수 의미
	CyXChart0.OpenData("COD_Values", 0, mon_cnt);
	CyXChart0.OpenData("COD_Colors", 0, mon_cnt);
/*
	CyXChart0.series[0].color = "#FBAFC2";
	CyXChart0.Series[0].YAxis = 0; 							//어떤 Y축을 사용할지를 나타탬
	CyXChart0.series[0].gallery = "Gallery_Bar";
	CyXChart0.Series[0].CylSides = 24;
	
	CyXChart0.series[1].color = "#FBE194";
	CyXChart0.Series[1].YAxis = 0; 							//어떤 Y축을 사용할지를 나타탬
	CyXChart0.series[1].gallery = "Gallery_Bar";
	CyXChart0.Series[1].CylSides = 24;
*/
	CyXChart0.series[0].color = "#7896d6";
	CyXChart0.Series[0].YAxis = 1; 							//어떤 Y축을 사용할지를 나타탬
	CyXChart0.series[0].gallery = 1; 							//꺾은선 그래프
	CyXChart0.series[0].gallery = "Gallery_Lines";

	CyXChart0.series[0].LineWidth = 2;
	
	//X축 값 설정  Axis[2]는 X축을 나타냄(label)
	for(var i=0 ; i < mon_cnt ; i++){
		CyXChart0.Axis[2].Label(i) = i*10 + "~" + (i+1)*10 + "점";
	}
	
	//실제 데이타를 이용해 그래프를 그려주는 부분 (RowCount / ColCount 등을 이용)
	var NULL_VAL = "1.0E+308";	// data=0 인경우 표시안함
	for (var i=0 ; i < mon_cnt ; i++)
	{
		//CyXChart0.Value(0,i) = ds_trend1.GetColumn(i, "col1");
		//CyXChart0.Value(1,i) = ds_trend1.GetColumn(i, "col2");
		CyXChart0.Value(0,i) = ds_score_level.GetColumn(0, i) + " 명";
	}	


	//CyXChart0.Series[0].PointLabels = false;	
	//CyXChart0.Series[1].PointLabels = false;	

	//Legend 표시
	//CyXChart0.Series[0].Legend = "Plan       ,";
	//CyXChart0.Series[1].Legend = "Perf.      ,";
	CyXChart0.Series[0].Legend = "%";

	CyXChart0.SerLegBoxObj.visible = true;
	CyXChart0.SerLegBoxObj.height = "10";
//	CyXChart0.SerLegBoxObj.Docked = "Docked_Top";

	CyXChart0.SerLegBoxObj.Docked = "1028"; 	//top:256, right:515"

	CyXChart0.SerLegBoxObj.top = "15";
	CyXChart0.SerLegBoxObj.left = "20";
	CyXChart0.SerLegBoxObj.width  = "220";
	CyXChart0.SerLegBoxObj.height = "22";


	//Chart 외곽 Gradient 설정
	var BK0Gradient = CreateObject("ChartFX.ClientServer.GradientBack.62");
		
	BK0Gradient.Type = "GradientType_BackwardDiagonal";
	BK0Gradient.Colors.Item(0) = "#FFFFFF";
	BK0Gradient.Colors.Item(1) = "#6B9CC9";
	BK0Gradient.Position.Item(0) = 0;
	BK0Gradient.Position.Item(1) = 0.5;
	BK0Gradient.Position.Item(2) = 1;
	BK0Gradient.Factor.Item(0) = 0;
	BK0Gradient.Factor.Item(1) = 0.5;
	BK0Gradient.Factor.Item(2) = 1;
	
	CyXChart0.BackObject = BK0Gradient;


	//Chart 내부 Gradient 설정
	var BK1Gradient = CreateObject("ChartFX.ClientServer.GradientBack.62");
		
	BK1Gradient.Type = "GradientType_BackwardDiagonal";
	BK1Gradient.Colors.Item(0) = "#FFFFFF";
	BK1Gradient.Colors.Item(1) = "#BDD8F2";
	BK1Gradient.Position.Item(0) = 0;
	BK1Gradient.Position.Item(1) = 0.5;
	BK1Gradient.Position.Item(2) = 1;
	BK1Gradient.Factor.Item(0) = 0;
	BK1Gradient.Factor.Item(1) = 0.5;
	BK1Gradient.Factor.Item(2) = 1;
	
	CyXChart0.InsideBackObject = BK1Gradient;

	CyXChart0.CloseData("COD_Values");
	CyXChart0.CloseData("COD_Colors");	
}


function CyXChart4_Load()
{
	var mon_cnt = ds_Trend.GetRowCount() -2;

	CyXChart4.TopGap = "10";						//차트 안쪽 상단 여백
	CyXChart4.LeftGap = "10";						//차트 안쪽 왼쪽 여백
	CyXChart4.RightGap = "10";					//차트 안쪽 오른쪽 여백
	CyXChart4.BottomGap = "10";					//차트 안쪽 하단 여백

	CyXChart4.ClearData(1);						//Clear all numeric values set with the YValue or Value properties
		
	CyXChart4.Stacked = 0;

	//Chart 외곽 영역 색상
	CyXChart4.BackColor = "#f2f2fe";
	
	//Chart 내부 색상
	CyXChart4.InsideColor ="#ffffff";					// 안쪽
	
	CyXChart4.AxisY.LabelsFormat.Decimals = 0;
	CyXChart4.AxisY.LabelsFormat.Format = "AxisFormat_Number";
	CyXChart4.AxisY.AutoScale = True;

	//CyXChart4.PointLabels = True;

	//X축의 갯수 설정
	CyXChart4.OpenData("COD_Values", 1, mon_cnt);
	CyXChart4.OpenData("COD_Colors", 1, mon_cnt);

	CyXChart4.Gallery = "Gallery_Pie";

	CyXChart4.Chart3D = true;

	
	CyXChart4.MultipleColors = true;		

	var objchart  ;
	
	//X축 값 설정  Axis[2]는 X축을 나타냄(label)
	for(var i=0 ; i < mon_cnt ; i++){
		CyXChart4.Axis[2].Label(i) = ds_right.getcolumn(i,"col0");
		CyXChart4.Legend(i) = ds_right.getcolumn(i,"col0");
	}

	//실제 데이타를 이용해 그래프를 그려주는 부분 (RowCount / ColCount 등을 이용)
	var NULL_VAL = "1.0E+308";	// data=0 인경우 표시안함
	for (var i=0 ; i < mon_cnt ; i++)
	{
		CyXChart4.Value(0,i) = ds_Trend.GetColumn(i, "col1");
		CyXChart4.Point.Item(0,i).color = ds_color.GetColumn(i, "data"); //GetBarColor(i);	
	}	  
   
	CyXChart4.LegendBoxObj.visible = true;


	//Chart  Gradient 설정
	var BK1Gradient = CreateObject("ChartFX.ClientServer.GradientBack.62");
		
	BK1Gradient.Type = "GradientType_BackwardDiagonal";
	BK1Gradient.Colors.Item(0) = "#FFFFFF";
	BK1Gradient.Colors.Item(1) = "#6969A0";
	BK1Gradient.Position.Item(0) = 0;
	BK1Gradient.Position.Item(1) = 0.5;
	BK1Gradient.Position.Item(2) = 1;
	BK1Gradient.Factor.Item(0) = 0;
	BK1Gradient.Factor.Item(1) = 0.5;
	BK1Gradient.Factor.Item(2) = 1;
	
	CyXChart4.BackObject = BK1Gradient;


	CyXChart4.CloseData("COD_Values");
	CyXChart4.CloseData("COD_Colors");
}

