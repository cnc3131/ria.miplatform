function changeDayColor(obj) {

	for(day=1;day<32;day++){
		//요일 =체크를 의해 날짜 생성
		var date="01";
		if(day<10){
			date = "0" + day;
		}else{
			date = day;
		}
		date = SubStr(obj.Value,0,6) + date;
		
		if(GetDay(DateTime(date)) == 0 ) {
			gdDailyRate.SetCellProp("body",(day + 1),"Color","Red"); 
		} else if(GetDay(DateTime(date)) == 6 ) {
			gdDailyRate.SetCellProp("body",(day + 1),"Color","Blue"); 
		}else{
			gdDailyRate.SetCellProp("body",(day + 1),"Color","Black"); 
		}
	}
}


function setProgress()
{
	var plan_rate = ds_condition.GetColumn(ds_condition.row, "PLAN_RATE");
	var plan_rate_idx = 6 + parseInt(parseInt(plan_rate)/2);
	var real_rate = ds_condition.GetColumn(ds_condition.row, "REAL_RATE");
	var real_rate_idx = 62 + parseInt(parseInt(real_rate)/2);
	
	var cellCnt = grd_type2.GetCellCount("body");
	for(var idx=0; idx<cellCnt; idx++)
	{
		// 윗줄 [7-56]
		if(idx>6 && idx<57)
		{
			if( plan_rate_idx>idx){
				grd_type2.SetCellProp("body", idx, "bkColor", "green");
				grd_type2.SetCellProp("body", idx, "bkColor2", "green");
			}
		}
		// 아랫줄 [63-112]
		else if(idx>62 && idx<113)
		{
			if( real_rate_idx>idx){
				grd_type2.SetCellProp("body", idx, "bkColor", "orange");
				grd_type2.SetCellProp("body", idx, "bkColor2", "orange");
			}
		}
		else
		{
			continue;
		}
		
		//trace("Cell["+idx+"] :: ["+grd_type2.GetCellProp("body", idx, "text")+"]");
	}
}

