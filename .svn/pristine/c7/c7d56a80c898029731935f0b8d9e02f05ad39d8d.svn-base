// 사용하지 않음.............................................
function changeMonth(obj){
		if(obj.Id == "btnNextMonth"){
			var nMonth = fnc_LastDay(AddMonth(calBasicDt.Value,1));
			if(SubStr(today(),0,6) < subStr(nMonth,0,6) )
				return;
			else if (SubStr(today(),0,6) == subStr(nMonth,0,6))
				calBasicDt.Value = AddDate(today(), -1);
			else
				calBasicDt.Value = nMonth;
				
		}else{
			calBasicDt.Value=fnc_LastDay(AddMonth(calBasicDt.Value,-1));
		}
}

// 프로젝트 상세보기..........................................
function grd_type1_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if(nCell==12)
	{
		G_ProjectName = ds_condition.GetColumn(ds_condition.row, "PRJ_NM");
		var menuRow = gds_menu.FindRow("MENUNAME", obj.text );
		
		if(menuRow <0) return;
		gds_menu.row  = menuRow;
		
		global.Frame_Left.pdiv_menu.Open_url( menuRow );
	}
}
