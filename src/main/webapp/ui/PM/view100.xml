		// case 400:[x,y] [103,163]

function setMaxView(arg_type)
{
	var jointLeft 	= 0;
	var jointTop 	= 0;
	
	switch(arg_type)
	{
		case 100:
			jointLeft	= 887;
			jointTop	= 434;
			sp_jointTop.left = jointLeft;
			sp_jointTop.top = jointTop;
			div_report.resizeAll(arg_type, 887, 434);
			div_result.resizeAll(arg_type, 103, 434);
			div_schedule.resizeAll(arg_type, 887, 163);
			div_process.resizeAll(arg_type, 103, 163);
			break;
		case 200:
			jointLeft	= 103;
			jointTop	= 434;
			sp_jointTop.left = jointLeft;
			sp_jointTop.top = jointTop;
			div_report.resizeAll(arg_type, 887, 434);
			div_result.resizeAll(arg_type, 103, 434);
			div_schedule.resizeAll(arg_type, 887, 163);
			div_process.resizeAll(arg_type, 103, 163);
			break;
		case 300:
			jointLeft	= 887;
			jointTop	= 163;
			sp_jointTop.left = jointLeft;
			sp_jointTop.top = jointTop;
			div_report.resizeAll(arg_type, 887, 434);
			div_result.resizeAll(arg_type, 103, 434);
			div_schedule.resizeAll(arg_type, 887, 163);
			div_process.resizeAll(arg_type, 103, 163);
			break;
		case 400:
			jointLeft	= 103;
			jointTop	= 163;
			sp_jointTop.left = jointLeft;
			sp_jointTop.top = jointTop;
			div_report.resizeAll(arg_type, 887, 434);
			div_result.resizeAll(arg_type, 103, 434);
			div_schedule.resizeAll(arg_type, 887, 163);
			div_process.resizeAll(arg_type, 103, 163);
			break;
		case 500:
			jointLeft	= 103;
			jointTop	= 559;
			sp_left_low_OnEndMove(sp_left_low, jointTop, 0);
			sp_jointTop.left = jointLeft;
			sp_jointTop.top = jointTop;
			break;
		default:	// 600
			jointLeft	= 887;
			jointTop	= 559;
			sp_left_low_OnEndMove(sp_left_low, jointTop, 0);
			sp_jointTop.left = jointLeft;
			sp_jointTop.top = jointTop;
			break;
	}
	adjustSplitMaxMinPos();

}

var title_height = 30;
function adjustSplitMaxMinPos()
{
	sp_left.MaxPos 	= sp_left_low.Top - title_height;
	sp_right.MaxPos = sp_left_low.Top - title_height;

	sp_left_low.MinPos 	= sp_left.Bottom + title_height;
	sp_right_low.MinPos = sp_left.Bottom + title_height;
}

