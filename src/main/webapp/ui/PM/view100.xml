// 이전 위치값.
function GetSplitPos(obj)
{
	var pos = 0;
	if( obj.Direct == "HORZ")
		pos = ( obj.right + obj.left ) / 2;
	else
		pos = ( obj.bottom + obj.top ) / 2;
	
	//trace(obj.id+".direct = "+ obj.Direct + ":: pos="+pos + ", point(left="+obj.left+", right="+obj.right+", top="+obj.top+", bottom="+obj.bottom+")");
	return pos;
}

// 상하이동 위치 조정
function sp_left_OnEndMove(obj, nPos, nDelta)
{
	var endPos = 0;
	if(nDelta==0)
		endPos = GetSplitPos(obj);
	
	div_report.resetBottom(nPos - endPos);
	div_schedule.resetBottom(nPos - endPos);
	div_result.resetBottom(-(nPos-endPos));
	div_process.resetBottom(-(nPos-endPos));
	
	adjustSplitMaxMinPos();
	trace(obj.id + "_OnEndMove(이전 Y:"+nPos+") : {이동값:"+endPos+"}"  );
}

// 좌우이동 위치 조정
function sp_top_OnEndMove(obj, nPos, nDelta)
{
	var endPos = 0;
	if(nDelta==0)
		endPos = GetSplitPos(obj);

	div_report.resetRight(nPos - endPos);
	div_result.resetRight(nPos - endPos);
	div_schedule.resetRight(-(nPos-endPos));
	div_process.resetRight(-(nPos-endPos));
	
	trace(obj.id + "_OnEndMove(이전 X:"+nPos+") : {이동값:"+endPos+"}"  );
}

// 하단 상하이동 위치 조정
function sp_left_low_OnEndMove(obj, nPos, nDelta)
{
	var endPos = 0;
	if(nDelta==0)
		endPos = GetSplitPos(obj);

	div_result.resetBottom(nPos - endPos);
	div_process.resetBottom(nPos - endPos);
	div_dangerous.resetBottom(-(nPos-endPos));

	adjustSplitMaxMinPos();
}

