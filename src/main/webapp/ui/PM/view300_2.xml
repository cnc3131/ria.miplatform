}


function CyXChart4_Load()
{
	var mon_cnt = ds_Trend.GetRowCount() -2;

	CyXChart4.TopGap = "10";						//차트 안쪽 상단 여백
	CyXChart4.LeftGap = "10";						//차트 안쪽 왼쪽 여백
	CyXChart4.RightGap = "10";					//차트 안쪽 오른쪽 여백
	CyXChart4.BottomGap = "10";					//차트 안쪽 하단 여백

	CyXChart4.ClearData(1);						//Clear all numeric values set with the YValue or Value properties
		
	CyXChart4.Stacked = 0;

	//Chart 외곽 영역 색상
	CyXChart4.BackColor = "#f2f2fe";
	
	//Chart 내부 색상
	CyXChart4.InsideColor ="#ffffff";					// 안쪽
	
	CyXChart4.AxisY.LabelsFormat.Decimals = 0;
	CyXChart4.AxisY.LabelsFormat.Format = "AxisFormat_Number";
	CyXChart4.AxisY.AutoScale = True;

	//CyXChart4.PointLabels = True;

	//X축의 갯수 설정
	CyXChart4.OpenData("COD_Values", 1, mon_cnt);
	CyXChart4.OpenData("COD_Colors", 1, mon_cnt);

	CyXChart4.Gallery = "Gallery_Pie";

	CyXChart4.Chart3D = true;

	
	CyXChart4.MultipleColors = true;		

	var objchart  ;
	
	//X축 값 설정  Axis[2]는 X축을 나타냄(label)
	for(var i=0 ; i < mon_cnt ; i++){
		CyXChart4.Axis[2].Label(i) = ds_right.getcolumn(i,"col0");
		CyXChart4.Legend(i) = ds_right.getcolumn(i,"col0");
	}

	//실제 데이타를 이용해 그래프를 그려주는 부분 (RowCount / ColCount 등을 이용)
	var NULL_VAL = "1.0E+308";	// data=0 인경우 표시안함
	for (var i=0 ; i < mon_cnt ; i++)
	{
		CyXChart4.Value(0,i) = ds_Trend.GetColumn(i, "col1");
		CyXChart4.Point.Item(0,i).color = ds_color.GetColumn(i, "data"); //GetBarColor(i);	
	}	  
   
	CyXChart4.LegendBoxObj.visible = true;


	//Chart  Gradient 설정
	var BK1Gradient = CreateObject("ChartFX.ClientServer.GradientBack.62");
		
	BK1Gradient.Type = "GradientType_BackwardDiagonal";
	BK1Gradient.Colors.Item(0) = "#FFFFFF";
	BK1Gradient.Colors.Item(1) = "#6969A0";
	BK1Gradient.Position.Item(0) = 0;
	BK1Gradient.Position.Item(1) = 0.5;
	BK1Gradient.Position.Item(2) = 1;
	BK1Gradient.Factor.Item(0) = 0;
	BK1Gradient.Factor.Item(1) = 0.5;
	BK1Gradient.Factor.Item(2) = 1;
	
	CyXChart4.BackObject = BK1Gradient;


	CyXChart4.CloseData("COD_Values");
	CyXChart4.CloseData("COD_Colors");
}

